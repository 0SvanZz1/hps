<%= form_with(model: estadia, local: true) do |form| %>
  <% if estadia.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(estadia.errors.count, "error") %> prohibited this estadia from being saved:</h2>

      <ul>
      <% estadia.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :vagas_codvaga %>
    <%= form.number_field :vagas_codvaga, id: :estadia_vagas_codvaga %>
  </div>

  <div class="field">
    <%= form.label :loja_codestab %>
    <%= form.number_field :loja_codestab, id: :estadia_loja_codestab %>
  </div>

  <div class="field">
    <%= form.label :usuarios_id %>
    <%= form.number_field :usuarios_id, id: :estadia_usuarios_id %>
  </div>

  <div class="field">
    <%= form.label :veiculos_codveic %>
    <%= form.number_field :veiculos_codveic, id: :estadia_veiculos_codveic %>
  </div>

  <div class="field">
    <%= form.label :clientes_codclie %>
    <%= form.number_field :clientes_codclie, id: :estadia_clientes_codclie %>
  </div>

  <div class="field">
    <%= form.label :data_entrada %>
    <%= form.date_select :data_entrada, id: :estadia_data_entrada %>
  </div>

  <div class="field">
    <%= form.label :data_saida %>
    <%= form.date_select :data_saida, id: :estadia_data_saida %>
  </div>

  <div class="field">
    <%= form.label :valor %>
