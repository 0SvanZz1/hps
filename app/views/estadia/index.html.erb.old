<p id="notice"><%= notice %></p>

<h1>Estadia</h1>

<table>
  <thead>
    <tr>
      <th>Vagas codvaga</th>
      <th>Loja codestab</th>
      <th>Usuarios</th>
      <th>Veiculos codveic</th>
      <th>Clientes codclie</th>
      <th>Data entrada</th>
      <th>Data saida</th>
      <th>Valor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estadia.each do |estadia| %>
      <tr>
        <td><%= estadia.vagas_codvaga %></td>
        <td><%= estadia.loja_codestab %></td>
        <td><%= estadia.usuarios_id %></td>
        <td><%= estadia.veiculos_codveic %></td>
        <td><%= estadia.clientes_codclie %></td>
        <td><%= estadia.data_entrada %></td>
        <td><%= estadia.data_saida %></td>
        <td><%= estadia.valor %></td>
        <td><%= link_to 'Show', estadia %></td>
        <td><%= link_to 'Edit', edit_estadia_path(estadia) %></td>
        <td><%= link_to 'Destroy', estadia, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New estadia', new_estadia_path %>
